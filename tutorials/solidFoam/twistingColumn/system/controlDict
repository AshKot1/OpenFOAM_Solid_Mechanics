FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application solidFoam;

startFrom latestTime;

startTime 0;

stopAt endTime;

endTime 0.25;

timeStepping variable;

cfl 0.3;

deltaT 0;

writeControl runTime;

writeInterval 0.01;

purgeWrite 0;

writeFormat ascii;

writePrecision 8;

writeCompression off;

timeFormat general;

timePrecision 6;

graphFormat raw;

runTimeModifiable false;

